{
    "name": "qmachard/atomic-design-bundle",
    "type": "symfony-bundle",
    "description": "Atomic Design System for Symfony.",
    "keywords": [
        "atomic",
        "design",
        "system",
        "html",
        "integration",
        "front-end",
        "css"
    ],
    "homepage": "https://github.com/qmachard/atomic-design-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Quentin Machard",
            "homepage": "http://www.quentinmachard.fr"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/framework-bundle": "^4.4||^5.0",
        "symfony/webpack-encore-bundle": "^1.8"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "QuentinMachard\\Bundle\\AtomicDesignBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\QuentinMachard\\Bundle\\AtomicDesignBundle\\": "spec/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "src"
        }
    ],
    "require-dev": {
        "phpspec/phpspec": "^7.0"
    }
}
