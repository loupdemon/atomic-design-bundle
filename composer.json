{
    "name": "qmachard/atomic-design-bundle",
    "type": "symfony-bundle",
    "description": "Atomic Design System for Symfony.",
    "keywords": [
        "atomic",
        "design",
        "system",
        "html",
        "integration",
        "front-end",
        "css"
    ],
    "homepage": "https://github.com/qmachard/atomic-design-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Quentin Machard",
            "homepage": "http://www.quentinmachard.fr"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "symfony/framework-bundle": "^4.4|^5.0",
        "sensio/framework-extra-bundle": "^4.4|^5.1|^6.0",
        "symfony/twig-bundle": "^4.4|^5.2",
        "symfony/webpack-encore-bundle": "^1.8"
    },
    "require-dev": {
        "phpspec/phpspec": "^7.0",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "QuentinMachard\\Bundle\\AtomicDesignBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\QuentinMachard\\Bundle\\AtomicDesignBundle\\": "spec/",
            "QuentinMachard\\Bundle\\AtomicDesignBundle\\Tests\\": "tests/",
            "QuentinMachard\\Bundle\\AtomicDesignBundle\\Tests\\App\\": "tests/app/src/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "src"
        }
    ]
}
